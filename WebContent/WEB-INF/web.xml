<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>DS</display-name>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>DS.main.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/IndexServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DetailServlet</servlet-name>
    <servlet-class>DS.main.servlet.DetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DetailServlet</servlet-name>
    <url-pattern>/DetailServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EventsServlet</servlet-name>
    <servlet-class>DS.main.servlet.EventsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventsServlet</servlet-name>
    <url-pattern>/EventsServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PeriodServlet</servlet-name>
    <servlet-class>DS.main.servlet.PeriodServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PeriodServlet</servlet-name>
    <url-pattern>/PeriodServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FacetServlet</servlet-name>
    <servlet-class>DS.main.servlet.FacetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FacetServlet</servlet-name>
    <url-pattern>/FacetServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>